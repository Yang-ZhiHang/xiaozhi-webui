{
  "name": "xiaozhi-webui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\" --names \"web,api\" --prefix-colors \"cyan,yellow\"",
    "dev:frontend": "vite --host",
    "dev:backend": "cd backend && uv run main.py",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "less": "^4.2.2",
    "less-loader": "^12.2.0",
    "pinia": "^3.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.0",
    "element-plus": "^2.9.11",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.8.0",
    "unplugin-element-plus": "^0.10.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
